{
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "globals": {
        "$": true,
        "BulmaFormHandler": true,
        "grecaptcha": true
    },
    "rules": {
        "prettier/prettier": ["error"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    }
}
